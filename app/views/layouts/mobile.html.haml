!!!5
%html
  %head
    %title
      Unseen
      ="- #{@page_title}" if @page_title.present?
    %meta{ :name => "viewport", :content => "width=device-width, initial-scale=1.0" }      
    -if Rails.env.development?
      =stylesheet_link_tag("mobile", "http://dl.dropbox.com/u/292336/unseen/apercu.css")
    -else
      =stylesheet_link_tag("mobile", "http://f.fontdeck.com/s/css/SHL1Q62RRv8EROgGSyefc+MAifY/unseen.yoomee.com/16878.css", "http://f.fontdeck.com/s/css/SHL1Q62RRv8EROgGSyefc+MAifY/www.unseenamsterdam.com/10817.css", "http://f.fontdeck.com/s/css/SHL1Q62RRv8EROgGSyefc+MAifY/unseenamsterdam.com/10817.css")
      
    =javascript_include_tag("mobile")
    =csrf_meta_tags
    =yield :head
    =google_analytics_js
  -body_tag do
    =yield_with_bootstrap(render_flash)
    =yield_with_bootstrap(yield)